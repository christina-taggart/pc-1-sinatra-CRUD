<form action="/notes/<%= @note.id %>" method="post">
  <input type="hidden" name="_method" value="put">
  Note Title: <input type="text" name="title" value="<%= @note.title %>">
  Note Content: <textarea rows="4" cols="50" name="content"><%= @note.content %></textarea>
  <input type="submit" value="submit">
</form>