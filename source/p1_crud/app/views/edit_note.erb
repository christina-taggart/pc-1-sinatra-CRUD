<form action="/notes/<%= session[:note_to_edit].id %>" method="post">
  <input type="hidden" name="_method" value="put">
  Note Title: <input type="text" name="title" value="<%= session[:note_to_edit].title %>">
  Note Content: <textarea rows="4" cols="50" name="content"><%= session[:note_to_edit].content %></textarea>
  <input type="submit" value="submit">
</form>