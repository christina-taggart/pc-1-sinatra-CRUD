<div class="container">
  <h1>Welcome to NoteMaker Pro!</h1>
  <form action="/" method="post">
    <input type="text" name="title" value="Note Title"><br>
    <input type="text" name="content" value="Note Content">
    <button type="button">Submit!</button>
  </form>
  <% @note.each do |note| %>
  <p id="title"><%= note.title %></p>
  <p><%= note.content %></p>
  <% end %>
</div>
